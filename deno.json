{
	"fmt": {
		"useTabs": true,
		"indentWidth": 4
	},
	"imports": {
		"@discordjs/core": "https://esm.sh/@discordjs/core@1.2.0/http-only",
		"@discordjs/formatters": "https://esm.sh/@discordjs/formatters@0.4.0",
		"@discordjs/rest": "https://esm.sh/@discordjs/rest@2.3.0",
		"@std/encoding": "jsr:@std/encoding@^0.224.3",
		"@std/fs": "jsr:@std/fs@^0.229.3",
		"@std/http": "jsr:@std/http@^0.224.5",
		"tweetnacl": "npm:tweetnacl@1.0.3"
	},
	"deploy": {
		"entrypoint": "main.ts",
		"include": [
			"commands/",
			"manifest.gen.ts",
			"deno.json",
			"deno.lock",
			"main.ts",
			"types.d.ts",
			"utils.ts"
		]
	},
	"tasks": {
		"build": "deno run --allow-run --allow-read --allow-write build.ts",
		"dev": "deno task build && deno run --allow-net --allow-env --watch=commands/,deno.json,main.ts,manifest.gen.ts,utils.ts, --env main.ts"
	}
}
